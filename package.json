{
  "name": "acronyms",
  "dependencies": {
    "@creativebulma/bulma-tooltip": "^1.2.0",
    "@fortawesome/fontawesome-free": "^6.4.2",
    "@vueuse/core": "^10.4.1",
    "@vueuse/motion": "^2.0.0",
    "bulma": "^0.9.4",
    "pinia": "^2.1.6",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@babel/types": "^7.22.11",
    "@types/node": "^20.5.7",
    "@types/web-bluetooth": "^0.0.17",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "@vitejs/plugin-vue": "^4.3.3",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "concurrently": "^8.2.1",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "eslint-plugin-vuejs-accessibility": "^2.2.0",
    "get-port": "^7.0.0",
    "maildev": "^2.1.0",
    "prettier": "^3.0.2",
    "prettier-plugin-organize-attributes": "^1.0.0",
    "prettier-plugin-organize-imports": "^3.2.3",
    "sass": "^1.66.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.1.6",
    "vite": "^4.4.9",
    "vue-tsc": "^1.8.8"
  },
  "private": true,
  "scripts": {
    "check": "vue-tsc --noEmit && poetry run mypy .",
    "build": "vue-tsc --noEmit && poetry run mypy . && vite build && poetry build",
    "dev": "node --no-warnings --loader ts-node/esm scripts/serve_devsite.ts",
    "format": "prettier --check . && poetry run black --check .",
    "lint": "eslint --no-fix --ext ts,vue --max-warnings 0 src/frontend && poetry run bandit -ilr src && poetry run ruff check .",
    "serve": "vite build && poetry run acronyms --log-level info",
    "test": "poetry run pytest --build --cov"
  },
  "type": "module",
  "version": "0.1.0"
}
